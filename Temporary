#include<stdio.h>
#include<stdlib.h>

typedef struct head{
    
    struct radix * start;
    int count;
    
}head;

typedef struct radix{
    
    struct radix * next;
    int data;

}radix;

head * createlistheader(){
    
    head * nodepointer;
    nodepointer = (head*)malloc(sizeof(head));
    nodepointer->start = NULL; 
    nodepointer->count = 0;
    return nodepointer;
    
}

radix * createnode(int value){
    
    radix * node;
    node = (radix*) malloc (sizeof(radix));
    node->next = NULL;
    node->data = value;
    return node;
    
}

void insert(radix * node, head * nodepointer, int element){
        
    radix * temp;
    temp = nodepointer->start;
    
    if(nodepointer->start == NULL){
        
        nodepointer->start = node;
        return;
    }

    while(temp->next != NULL){
        
        temp = temp->next;
    }
    
    temp->next = node;
    return;
}

void display(radix * node){
    
    while(node != NULL){
        
        printf("%d\n", node->data);
        node = node->next;
    }
}

int main(){
 
    radix * node;
    head * nodepointer;
    int i, j, k = 1, temp, element, n;
    
    nodepointer = createlistheader();
    printf("Enter number of elements\n");

    node = createnode(103);
    insert(node, nodepointer, 103);
    node  = createnode(108);
    insert(node, nodepointer, 108);
    node  = createnode(124);
    insert(node, nodepointer, 124);
    node = createnode(117);
    insert(node, nodepointer, 117);
    node = createnode(105);
    insert(node, nodepointer, 105);

    
    display(nodepointer->start);
}
